<script>


    var script = {
        // 확인
        confirm: function (action = false) {
            (action == false) ? $("#confirm").hide() : $("#confirm").show();
            return this;
        },
        // 기본 백그라운드
        background: function (index = 0, action = false) {
            (action == false) ? $("#background").css('z-index', 0).hide() : $("#background").css('z-index', index).show();
            return this;
        },
        // 팝업
        popup: function (action = false) {
            (action == false) ? $("#popup").hide() : $("#popup").show();
            return this;
        },
        // 약관
        terms: function (action = false) {
            (action == false) ? $("#terms").hide() : $("#terms").show();
        },
        // 지역 배열
        area: function () {
            this.selects = [];
            $.each($("#popup .add .country"), (index, item) => {
                this.selects.push($(item).text().replace(" X ", ""));
            });
            return this;
        }
    }


    onscroll = (event) => {
        // 브랜드 소개 
        (window.scrollY > 0)? $(".intro .brand").css("top","-70px"):$(".intro .brand").css("top","0px");

        if (window.scrollY >= document.getElementById("main").offsetHeight) {
            $(".brand-list").addClass("brand-active");
        } else {
            $(".brand-list").removeClass("brand-active");
        }
        
        if (window.scrollY >= document.getElementById("main").offsetHeight + document.getElementById("brand-intro").offsetHeight - 100) {
            $(".service-list").addClass("service-active");
        } else {
            $(".service-list").removeClass("service-active");
        }
    };

    $(".swiper-container").carousel({
         width: window.innerWidth,
        height: window.innerHeight,
        speed: 3e3,
        needPrevAndNextBtnGroup: true,
        needPagination: true,
    });

    /***창업 문의 바로가기 **/
    $(".button").click(function () {
        script.background(9, true).popup(true)
        // $("#background").css('z-index',9).show();
        //   $("#popup").show();
    });

    /***창업 문의 닫기 **/
    $(".popup-close").click(function () {
        script.background(0, false).popup(false)
        // $("#background").css('z-index',0).hide();
        //   $("#popup").hide();
    });

    /*** 추가 버튼 */
    $("#popup .poup-input button").click(function () {
        if ($("#gugun1").val() == '') {
            alert("창업희망지역 선택해주세요");
            return false;
        } else if ($("#popup .add .ad-close").length > 2) {
            alert("창업희망지역 3곳만 선택 가능합니다.");
            return false;
        }
        script.area();
        let areaCheck = script.selects.indexOf(`${$("#sido1").val()}.${$("#gugun1").val()}`);
        if (areaCheck !== -1) return false;
        $("#popup .add").append(`<span class='country'>${$("#sido1").val()}.${$("#gugun1").val()} <span class='ad-close'>X</span> </span>`).show();
    });

    /*** 지역 삭제 **/
    $(document).on('click', '#popup .add .ad-close', function () {
        if ($("#popup .add .ad-close").length == 1) {
            $("#popup .add").hide();
        }
        ;
        $(this).parent().remove();
    })

    /**** 개인정보 취급동의 *****/
    $("#popup .tos").click(function () {
        // $("#background").css('z-index',19).show();
        // $("#terms").show();
        script.background(index = 19, action = true).terms(action = true)
        $("[name=terms-ck]").attr("checked", "true");
    })
    /** 이용야관 **/
    $("#terms .terms-close").click(function () {
